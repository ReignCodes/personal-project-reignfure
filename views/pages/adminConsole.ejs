<main>
    <div class="container">
        <div class="tableContainer">    
            <table class="adminResource">
                <tr>
                    <th>Resource Name</th>
                    <th>Type</th>
                    <th>Is Approved?</th>
                    <th>Review</th>
                    <th>Delete</th>
                </tr>
            
                <% data.forEach(resource => { %>
                    <%- include('../partials/resource', {resource: resource}) -%>
                            <% }); %>
                
            </table>
        </div>
        <div class="tableContainer">
            <table class="adminContact">
                <tr>
                    <th>Last Name</th>
                    <th>First Name</th>
                    <th>Volunteer</th>
                    <th>Contacted</th>
                    <th>Admin</th>
                    <th>Username</th>
                    <th>Read</th>
                    <th>Delete</th>
                </tr>
            
                <% data.forEach(contact => { %>
                    <%- include('../partials/contact', {contact: contact}) -%>
                            <% }); %>
                
            </table>
        </div>
    </div>
</main>